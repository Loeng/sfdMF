<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ekfans.base.tender.model">

	<!-- 合同表 -->
	<class name="TenderLog" table="TENDER_LOG" dynamic-update="false">
		<id name="id" column="ID" type="string" unsaved-value="null">
			<generator class="uuid.hex" />
		</id>
		<property name="tenderId" type="string" column="TENDER_ID" />
		<property name="creator" type="string" column="CREATOR" />
		<property name="creatorName" type="string" column="CREATOR_NAME" />
		<property name="name" type="string" column="NAME" />
		<property name="phone" type="string" column="PHONE" />
		<property name="mail" type="string" column="MAIL" />
		<property name="createTime" type="string" column="CREATE_TIME" />
	</class>

</hibernate-mapping>
