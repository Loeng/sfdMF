<!doctype html><html><head><meta name="renderer" content="webkit">	<meta http-equiv="X-UA-Compatible" content="IE=Edge">	<title>需求完成</title>		<script src="${webroot}/resources/plugin/datePicker/WdatePicker.js"></script>		<script src="${webroot}/resources/userCenter/personal/js/ckeditor/ckeditor.js"></script>		<script type="text/javascript" src="${webroot}/resources/commons/js/jquery.form.js"></script>		<script type="text/javascript" src="${webroot}/resources/commons/js/ajaxupload.js"></script>		<link href="${webroot}/resources/userCenter/commons/css/addGoods.css" rel="stylesheet" type="text/css">		<link href="${webroot}/resources/userCenter/commons/css/proAdd.css" rel="stylesheet" type="text/css">		<link href="${webroot}/resources/userCenter/commons/css/zizhi1.css" rel="stylesheet" type="text/css">		<script type="text/javascript" src="${webroot}/resources/commons/js/upload/upload.js"></script>		<link href="${webroot}/resources/commons/css/tools.css" rel="stylesheet" type="text/css">		<script type="text/javascript" src="${webroot}/resources/commons/js/tools.js"></script>		<script type="text/javascript" src="${webroot}/resources/plugin/datePicker/WdatePicker.js"></script>		<!--导入自定义Js-->		<script type="text/javascript" src="${webroot}/resources/userCenter/commons/js/supplyBuy/ylxqSupplyBuyOk.js"></script>		<style>		.formItem dt{height:28px;line-height:28px;}		</style></head><body>	<div class="contentTit budong borderColor">		<h3>需求完成</h3>		<a href="javascript:void(0);" class="btnBase btnSave" onclick="formSubmit();">提交</a>		<a href="javascript:void(0);" class="btnBase btnSave" onclick="goBack('${webroot}');">返回列表</a>		<div class="clear"></div>	</div>	<input type="hidden" id="ctxpath" value="${webroot}"/>	<div class="form borderColor">		#define(com.ekfans.base.order.model.SupplyBuy sb)		#define(List<com.ekfans.base.store.model.Intel> ilist)		<form action="${webroot}/store/supplyBuy/ylxqUpdate" name="addOk" method="post"  id="updateSupplyBuyForm">			<input type="hidden" name="id" value="${sb.id}"/>			<input type="hidden" name="storeId" value="${sb.storeId}"/>			<input type="hidden" name="type" id="type" value="${sb.type}"/>			<input type="hidden" name="productType" id="productType" value="${sb.productType}"/>			<input type="hidden" name="status" value="2"/>			<input type="hidden" name="createTime" value="${sb.createTime}"/>			<input type="hidden" name="note" value="${sb.note}"/>			<input type="hidden" name="orgId" value="${sb.orgId}"/>			<dl class="formItem">				<dt><em class="mustIco"></em>供求标题</dt>				<dd>					<input type="text" class="textBox box212" placeholder="请输入供求标题"  name="title"   value="${sb.title}" readonly/>					  <div class="clear"></div>					  <span id="titleSpan" class="errorText" style="display:none;"></span>				</dd>				<dd class="clear"></dd>			</dl>		  		  <dl class="formItem">				<dt><em class="mustIco"></em>联系人</dt>				<dd>					<input type="text" class="textBox box212" placeholder="请输入联系人"  name="contactName"  value="${sb.contactName}" readonly/>					  <div class="clear"></div>					  <span id="contactNameSpan" class="errorText" style="display:none;"></span>				</dd>				<dd class="clear"></dd>			</dl>						<dl class="formItem">				<dt><em class="mustIco"></em>联系电话</dt>				<dd>					<input type="text" class="textBox box212" placeholder="请输入联系方式"  name="contactPhone"  value="${sb.contactPhone}" readonly/>  				 <div class="clear"></div>  				<span id="contactPhoneSpan" class="errorText" style="display:none;"></span>				</dd>				<dd class="clear"></dd>			</dl>			<dl class="formItem">				<dt>有效时间</dt>				<dd>					<input name="endTime" type="text" class="textBox box212"  value="${sb.endTime}" readonly/>				</dd>										<dd class="clear"></dd>			</dl>			<dl class="formItem">				<dt>预估价</dt>				<dd>					<input name="futurePrices" value="${sb.futurePrices}" type="text" class="textBox box212" maxlength="19">				</dd>										<dd class="clear"></dd>			</dl>	<dl class="formItem" style="width:980px;">		    	<dt style="height:40px;line-height:40px;">资质名称</dt>		      <dd style="height:auto;">		      	#if(ilist==null || ilist.size()<=0)		      		<ul class="zizhiList">		      			<li title="选择你要认证的资质名称">选择你要认证的资质名称</li>		      			<div class="clear"></div>		      		</ul>		      	#else		      		<ul class="zizhiList">		      			#for(com.ekfans.base.store.model.Intel in : ilist)		      				<li title="${in.name}">${in.name}		      					<a href="javascript:void(0);" class="closeLi"></a>		      					<input type="hidden" name="intelId" value="${in.id}" />		      				</li>		      			#end		      		</ul>					      	#end		        <div class="clear"></div>		      </dd>		      <div class="clear"></div>		  	</dl>			<dl class="formItem formLine">				<dt>详细描述</dt>				<dd >					<textarea name="content" style="width:545px;" readonly>${sb.content}</textarea>				</dd> 				<dd class="clear"></dd>			</dl>				<dl class="formItem formLine">				<dt>备注</dt>				<dd >					<textarea name="note" style="width:545px;" maxlength="256" ></textarea>				</dd> 				<dd class="clear"></dd>			</dl>			<div class="clear"></div>		</form>		</div>		 <script type="text/javascript" src="${webroot}/resources/userCenter/commons/js/base.js"></script></body></html>