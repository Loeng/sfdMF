<!doctype html><html><head><meta name="renderer" content="webkit">	<meta http-equiv="X-UA-Compatible" content="IE=Edge">	<title>需求详情</title>		<script src="${webroot}/resources/plugin/datePicker/WdatePicker.js"></script>		<script src="${webroot}/resources/userCenter/personal/js/ckeditor/ckeditor.js"></script>		<script type="text/javascript" src="${webroot}/resources/commons/js/jquery.form.js"></script>		<script type="text/javascript" src="${webroot}/resources/commons/js/ajaxupload.js"></script>		<link href="${webroot}/resources/userCenter/commons/css/addGoods.css" rel="stylesheet" type="text/css">		<link href="${webroot}/resources/userCenter/commons/css/proAdd.css" rel="stylesheet" type="text/css">		<link href="${webroot}/resources/userCenter/commons/css/zizhi1.css" rel="stylesheet" type="text/css">		<script type="text/javascript" src="${webroot}/resources/commons/js/upload/upload.js"></script>		<link href="${webroot}/resources/commons/css/tools.css" rel="stylesheet" type="text/css">		<script type="text/javascript" src="${webroot}/resources/commons/js/tools.js"></script>		<script type="text/javascript" src="${webroot}/resources/plugin/datePicker/WdatePicker.js"></script>		<!--导入自定义Js-->		<script type="text/javascript" src="${webroot}/resources/userCenter/commons/js/supplyBuy/wfpSupplyBuyQuery.js"></script>		<style>		.formItem dt{height:28px;line-height:28px;}		</style></head><body>	<div class="contentTit budong borderColor">		<h3>供需详情</h3>		<a href="javascript:void(0);" class="btnBase btnSave" onclick="goBack('${webroot}');">返回列表</a>		<div class="clear"></div>	</div>	<input type="hidden" id="ctxpath" value="${webroot}"/>	<div class="form borderColor">		#define(com.ekfans.base.order.model.SupplyBuy sb)				#define(List<com.ekfans.base.store.model.Intel> ilist)			<input type="hidden" name="productType" id="productType" value="${sb.productType}"/>			<input type="hidden" name="type" id="type" value="${sb.type}"/>			<dl class="formItem">				<dt><em class="mustIco"></em>供求标题</dt>				<dd>					<input type="text" class="textBox box212" value="${sb.title}" />				</dd>				<dd class="clear"></dd>			</dl>		  		  <dl class="formItem">				<dt><em class="mustIco"></em>联系人</dt>				<dd>					<input type="text" class="textBox box212" value="${sb.contactName}" />					 				</dd>				<dd class="clear"></dd>			</dl>						<dl class="formItem">				<dt><em class="mustIco"></em>联系电话</dt>				<dd>					<input type="text" class="textBox box212"   value="${sb.contactPhone}"/>				</dd>				<dd class="clear"></dd>			</dl>			<dl class="formItem">				<dt>有效时间</dt>				<dd>					<input name="endTime" type="text" class="textBox box212" value="${sb.endTime}">				</dd>										<dd class="clear"></dd>			</dl>	<dl class="formItem" style="width:980px;">		    	<dt style="height:40px;line-height:40px;">资质名称</dt>		      <dd style="height:auto;">		      	#if(ilist==null || ilist.size()<=0)		      		<ul class="zizhiList">		      			<li title="选择你要认证的资质名称">选择你要认证的资质名称</li>		      			<div class="clear"></div>		      		</ul>		      	#else		      		<ul class="zizhiList">		      			#for(com.ekfans.base.store.model.Intel in : ilist)		      				<li title="${in.name}">${in.name}		      					<a href="javascript:void(0);" class="closeLi"></a>		      					<input type="hidden" name="intelId" value="${in.id}" />		      				</li>		      			#end		      		</ul>					      	#end		        <div class="clear"></div>		      </dd>		      <div class="clear"></div>		  	</dl>			<dl class="formItem formLine">				<dt>详细描述</dt>				<dd >					<textarea name="content" style="width:545px;">${sb.content}</textarea>				</dd> 				<dd class="clear"></dd>			</dl>					<div class="clear"></div>	</div>	</body></html>