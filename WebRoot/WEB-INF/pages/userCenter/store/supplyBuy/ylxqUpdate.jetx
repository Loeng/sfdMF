<!doctype html><html><head><meta name="renderer" content="webkit">	<meta http-equiv="X-UA-Compatible" content="IE=Edge">	<title>需求详情</title>		<script src="${webroot}/resources/plugin/datePicker/WdatePicker.js"></script>		<script src="${webroot}/resources/userCenter/personal/js/ckeditor/ckeditor.js"></script>		<script type="text/javascript" src="${webroot}/resources/commons/js/jquery.form.js"></script>		<script type="text/javascript" src="${webroot}/resources/commons/js/ajaxupload.js"></script>		<link href="${webroot}/resources/userCenter/commons/css/addGoods.css" rel="stylesheet" type="text/css">		<link href="${webroot}/resources/userCenter/commons/css/proAdd.css" rel="stylesheet" type="text/css">		<link href="${webroot}/resources/userCenter/commons/css/zizhi1.css" rel="stylesheet" type="text/css">		<script type="text/javascript" src="${webroot}/resources/commons/js/upload/upload.js"></script>		<link href="${webroot}/resources/commons/css/tools.css" rel="stylesheet" type="text/css">		<script type="text/javascript" src="${webroot}/resources/commons/js/tools.js"></script>		<script type="text/javascript" src="${webroot}/resources/plugin/datePicker/WdatePicker.js"></script>		<!--导入自定义Js-->		<script type="text/javascript" src="${webroot}/resources/userCenter/commons/js/supplyBuy/ylxqSupplyBuyUpdate.js"></script>		<style>		.formItem dt{height:28px;line-height:28px;}		.areaIdArea{width:100px;}		</style></head><body>	#define(com.ekfans.base.order.model.SupplyBuy sb)	#define(List<com.ekfans.base.product.model.ProductCategory> categories)	<div class="contentTit budong borderColor">		<h3>#if(sb.type == "0")供应信息修改#else求购信息修改#end</h3>		<a href="javascript:void(0);" class="btnBase btnSave" onclick="formSubmit();">保存</a>		<a href="javascript:void(0);" class="btnBase btnCancel" onclick="reset();">重置</a>		<a href="javascript:void(0);" class="btnBase btnSave" onclick="goBack('${webroot}');">返回列表</a>		<div class="clear"></div>	</div>	<input type="hidden" id="ctxpath" value="${webroot}"/>	<div class="form borderColor">		#define(List<com.ekfans.base.store.model.Intel> ilist)		<form action="${webroot}/store/supplyBuy/ylxqUpdate" name="addOk" method="post"  id="updateSupplyBuyForm">			<input type="hidden" name="id" value="${sb.id}"/>			<input type="hidden" name="storeId" value="${sb.storeId}"/>			<input type="hidden" name="type" id="type" value="${sb.type}"/>			<input type="hidden" name="productType" id="productType" value="${sb.productType}"/>			<input type="hidden" name="status" value="${sb.status}"/>			<input type="hidden" name="createTime" value="${sb.createTime}"/>			<input type="hidden" name="note" value="${sb.note}"/>			<input type="hidden" name="orgId" value="${sb.orgId}"/>			<dl class="formItem">				<dt><em class="mustIco"></em>原料分类</dt>				<dd>					<select name="categoryId" style="width:120px;"  id="categoryId">						#for(com.ekfans.base.product.model.ProductCategory pc :categories)							<option value="${pc.id}" #if(pc.id==sb.categoryId)  selected="selected" #end >${pc.name}</option>						#end					</select>					<span id="categorySapn" class="errorText" style="display:none;"></span>				</dd>				<dd class="clear"></dd>			</dl>			<dl class="clear"></dl>			<dl class="formItem">				<dt><em class="mustIco"></em>#if(sb.type == "0")供应标题#else求购标题#end</dt>				<dd>						<input type="text" class="textBox box212" value="${sb.title}"  name="title"  onblur="javascript:checkTitleBlur();" maxlength="64"/>					  	<div class="clear"></div>					  <span id="titleSpan" class="errorText" style="display:none;"></span>				</dd>				<dd class="clear"></dd>			</dl>		  		    <dl class="formItem">				<dt><em class="mustIco"></em>质量等级</dt>				<dd>						<input type="text" class="textBox box212" value="${sb.qualityLevel}"  name="qualityLevel"  onblur="javascript:qualityLevelBlur();" maxlength="64"/>					  	<div class="clear"></div>					  <span id="qualityLevelSpan" class="errorText" style="display:none;"></span>				</dd>				<dd class="clear"></dd>			</dl>			<dl class="formItem">				<dt><em class="mustIco"></em>计量单位</dt>				<dd>					<input type="text" class="textBox box212" value="${sb.unit}"  name="unit"  onblur="javascript:unitBlur();" maxlength="64"/>					  	<div class="clear"></div>					  <span id="unitSpan" class="errorText" style="display:none;"></span>				</dd>										<dd class="clear"></dd>			</dl>			<dl class="formItem">				<dt><em class="mustIco"></em>预估单价</dt>				<dd>					<input type="text" class="textBox box212" value="${sb.futurePrices}"  name="futurePrices"  placeholder="不填默认为待议" onblur="javascript:futurePricesBlur();" maxlength="64"/>					<dd id="">						<span id="unitShow">&nbsp;单位:${sb.unit}</span>					</dd> 				  	<div class="clear"></div>					<span id="futurePricesSpan" class="errorText" style="display:none;"></span>				</dd>						<dd class="clear"></dd>			</dl>		    <dl class="formItem">				<dt><em class="mustIco"></em>预估数量</dt>				<dd>						<input type="text" class="textBox box212" value="${sb.number}"  name="number"  onblur="javascript:checkNumberBlur();" maxlength="64"/>					  	<div class="clear"></div>					  <span id="numberSpan" class="errorText" style="display:none;"></span>				</dd>				<dd class="clear"></dd>			</dl>								    <dl class="formItem">				<dt><em class="mustIco"></em>联系人</dt>				<dd>					<input type="text" class="textBox box212" value="${sb.contactName}"  name="contactName"  onblur="javascript:checkContentNameBlur();" maxlength="64"/>					  <div class="clear"></div>					<span id="contactNameSpan" class="errorText" style="display:none;"></span>				</dd>				<dd class="clear"></dd>			</dl>						<dl class="formItem">				<dt><em class="mustIco"></em>联系电话</dt>				<dd>					<input type="text" class="textBox box212" value="${sb.contactPhone}" maxlength="11" onkeyup="this.value=this.value.replace(/\D/g,'')" onafterpaste="this.value=this.value.replace(/\D/g,'')" name="contactPhone"  onblur="javascript:checkContentPhoneBlur();" maxlength="16"/>  				 <div class="clear"></div>  				<span id="contactPhoneSpan" class="errorText" style="display:none;"></span>				</dd>				<dd class="clear"></dd>			</dl>						<dl class="formItem">				<dt><em class="mustIco"></em>交货方式</dt>				<dd>					<label><input name="deliveryType" value="0" type="radio" #if(sb.deliveryType==0) checked="checked" #end />自提 </label> 					<label><input name="deliveryType" value="1" type="radio" #if(sb.deliveryType==1) checked="checked" #end />送货上门 </label> 				</dd>				<dd class="clear"></dd>			</dl>						<dl class="formItem">				<dt><em class="mustIco"></em>交货地</dt>				<dd>				    #tag systemAreaSel("areaId",sb.areaId) #end					<span id="" class="errorText" style="display:none;"></span>				</dd>				<dd class="clear"></dd>				<dd style="min-height:0;margin-top:-28px;margin-left:94px;padding-right:10px;width:100px;"><span id="areaSpan" class="errorText" style="display:none;"></span></dd>				<dd class="clear"></dd>			</dl>			<input type="hidden" value="" name="destination" id="destination"/ >			<dl class="clear"></dl>			<dl class="formItem">				<dt>详细地址</dt>				<dd>					<input type="text" class="textBox box212" value="${sb.address}"  name="address"   maxlength="128"/>  				 <div class="clear"></div>  				<span id="contactPhoneSpan" class="errorText" style="display:none;"></span>				</dd>				<dd class="clear"></dd>			</dl>						<dl class="formItem">				<dt><em class="mustIco"></em>有效时间</dt>				<dd>					<input name="endTime" type="text" value="${sb.endTime}" class="textBox box212" onblur="javascript:checkEndTimeBlur();"  onclick="WdatePicker({dateFmt:'yyyy-MM-dd HH:mm:ss'})" maxlength="19">													  <div class="clear"></div>					<span id="endTimeSpan" class="errorText" style="display:none;"></span>				</dd>										<dd class="clear"></dd>			</dl>						<dl class="formItem" style="width:980px;">		    	<dt style="height:40px;line-height:40px;">资质名称</dt>		      <dd style="height:auto;">		      	#if(ilist==null || ilist.size()<=0)		      		<ul class="zizhiList">		      			<li title="选择你要认证的资质名称">选择你要认证的资质名称</li>		      			<div class="clear"></div>		      		</ul>		      	#else		      		<ul class="zizhiList">		      			#for(com.ekfans.base.store.model.Intel in : ilist)		      				<li title="${in.name}">${in.name}		      					<a href="javascript:void(0);" class="closeLi"></a>		      					<input type="hidden" name="intelId" value="${in.id}" />		      				</li>		      			#end		      		</ul>					      	#end		        <a href="javascript:void(0);" class="btnXuanze">请选择</a>		        <div class="clear"></div>		      </dd>		      <div class="clear"></div>		  	</dl>		  				<dl class="formItem formLine">				<dt>详细描述</dt>				<dd >					<textarea id="content"  name="content" style="width:545px;" maxlength="200">${sb.content}</textarea>				</dd> 				<dd class="clear"></dd>			</dl>					<div class="clear"></div>			</from>		</div>		<!--弹窗 css--><div class="alertLayerBg" style="display:none"></div><!--选择弹窗--><div class="alterBox"></div> <script type="text/javascript" src="${webroot}/resources/userCenter/commons/js/base.js"></script></body></html>