<!doctype html><html><head><meta name="renderer" content="webkit">	<meta http-equiv="X-UA-Compatible" content="IE=Edge">	<title>需求详情</title>		<script src="${webroot}/resources/plugin/datePicker/WdatePicker.js"></script>		<script src="${webroot}/resources/userCenter/personal/js/ckeditor/ckeditor.js"></script>		<script type="text/javascript" src="${webroot}/resources/commons/js/jquery.form.js"></script>		<script type="text/javascript" src="${webroot}/resources/commons/js/ajaxupload.js"></script>		<link href="${webroot}/resources/userCenter/commons/css/addGoods.css" rel="stylesheet" type="text/css">		<link href="${webroot}/resources/userCenter/commons/css/proAdd.css" rel="stylesheet" type="text/css">		<link href="${webroot}/resources/userCenter/commons/css/zizhi1.css" rel="stylesheet" type="text/css">		<script type="text/javascript" src="${webroot}/resources/commons/js/upload/upload.js"></script>		<link href="${webroot}/resources/commons/css/tools.css" rel="stylesheet" type="text/css">		<script type="text/javascript" src="${webroot}/resources/commons/js/tools.js"></script>		<script type="text/javascript" src="${webroot}/resources/plugin/datePicker/WdatePicker.js"></script>		<!--导入自定义Js-->		<script type="text/javascript" src="${webroot}/resources/userCenter/commons/js/supplyBuy/ylxqSupplyBuyQuery.js"></script>		<style>		.formItem dt{height:28px;line-height:28px;}		.areaIdArea{width:100px;}		</style></head><body>	#define(com.ekfans.base.order.model.SupplyBuy sb)	#define(List<com.ekfans.base.product.model.ProductCategory> categories)	<div class="contentTit budong borderColor">		<h3>#if(sb.type == "0")供应详情#else求购详情#end</h3>		<a href="javascript:void(0);" class="btnBase btnSave" onclick="goBack('${webroot}');">返回列表</a>		<div class="clear"></div>	</div>	<input type="hidden" id="ctxpath" value="${webroot}"/>	<div class="form borderColor">					#define(List<com.ekfans.base.store.model.Intel> ilist)			<input type="hidden" name="productType" id="productType" value="${sb.productType}"/>			<input type="hidden" name="type" id="type" value="${sb.type}"/>		  <dl class="formItem">				<dt><em class="mustIco"></em>原料分类</dt>				<dd>					<select name="categoryId" disabled id="categoryId" style="width:120px;">						#for(com.ekfans.base.product.model.ProductCategory pc:categories)							<option value="${pc.id}" #if(pc.id==sb.categoryId) selected #end >${pc.name}</option>						#end					</select>					<span id="categorySapn" class="errorText" style="display:none;"></span>				</dd>				<dd class="clear"></dd>			</dl>		  <dl class="clear"></dl>		  <dl class="formItem">				<dt><em class="mustIco"></em>#if(sb.type == "0")供应标题#else求购标题#end</dt>				<dd>					<input type="text" class="textBox box212" disabled value="${sb.title}" />				</dd>				<dd class="clear"></dd>			</dl>						<dl class="formItem">				<dt><em class="mustIco"></em>质量等级</dt>				<dd>						<input type="text" class="textBox box212"  value="${sb.qualityLevel}"  disabled name="qualityLevel"   maxlength="64"/>					  	<div class="clear"></div>					  <span id="qualityLevelSpan" class="errorText" style="display:none;"></span>				</dd>				<dd class="clear"></dd>			</dl>					  <dl class="formItem">				<dt><em class="mustIco"></em>计价单位</dt>				<dd>					<input type="text" class="textBox box212" value="${sb.unit}" disabled  name="unit"   maxlength="64"/>					  	<div class="clear"></div>					  <span id="unitSpan" class="errorText" style="display:none;"></span>				</dd>										<dd class="clear"></dd>			</dl>					  <dl class="formItem">				<dt><em class="mustIco"></em>预估单价</dt>				<dd>				#if(sb.futurePrices==null||sb.futurePrices==" ")					<input type="text" class="textBox box212"value="待议" disabled name="futurePrices"   maxlength="64"/>					 #else 					 <input type="text" class="textBox box212"value="${sb.futurePrices}" disabled name="futurePrices"   maxlength="64"/>					 #end					  	<div class="clear"></div>					  <span id="futurePricesSpan" class="errorText" style="display:none;"></span>				</dd>						<dd>&nbsp;单位:${sb.unit}</dd> 								<dd class="clear"></dd>			</dl>			<dl class="formItem">				<dt><em class="mustIco"></em>预估数量</dt>				<dd>						<input type="text" class="textBox box212" value="${sb.number}" disabled name="number"   maxlength="64"/>					  	<div class="clear"></div>					  <span id="numberSpan" class="errorText" style="display:none;"></span>				</dd>				<dd class="clear"></dd>			</dl>		  		  		  <dl class="formItem">				<dt><em class="mustIco"></em>联系人</dt>				<dd>					<input type="text" class="textBox box212" disabled value="${sb.contactName}" />					 				</dd>				<dd class="clear"></dd>			</dl>						<dl class="formItem">				<dt><em class="mustIco"></em>联系电话</dt>				<dd>					<input type="text" class="textBox box212" disabled  value="${sb.contactPhone}"/>				</dd>				<dd class="clear"></dd>			</dl>						<dl class="formItem">				<dt><em class="mustIco"></em>交货方式</dt>				<dd>					<label><input name="deliveryType" disabled type="radio" #if(sb.deliveryType==0) checked="checked" #end/>自提 </label> 					<label><input name="deliveryType" disabled type="radio" #if(sb.deliveryType==1) checked="checked" #end />送货上门 </label> 				</dd>				<dd class="clear"></dd>			</dl>						<dl class="formItem" >				<dt><em class="mustIco"></em>交货地</dt>				<dd >				    <input type="text" class="textBox box212" value="${sb.destination}" disabled name="destination"   maxlength="32"/>					<span id="" class="errorText" style="display:none;"></span>				</dd>				<dd class="clear"></dd>			</dl>			<dl class="clear"></dl>			<dl class="formItem">				<dt>详细地址</dt>				<dd>					<input type="text" class="textBox box212" value="${sb.address}" disabled name="address"   maxlength="16"/>  				 <div class="clear"></div>  				<span id="contactPhoneSpan" class="errorText" style="display:none;"></span>				</dd>				<dd class="clear"></dd>			</dl>						<dl class="formItem">				<dt>有效时间</dt>				<dd>					<input name="endTime" type="text" class="textBox box212" disabled value="${sb.endTime}">				</dd>										<dd class="clear"></dd>			</dl>			<dl class="formItem" style="width:980px;">		    	<dt style="height:40px;line-height:40px;">资质名称</dt>		      <dd style="height:auto;">		      	#if(ilist==null || ilist.size()<=0)		      		<ul class="zizhiList">		      			<li title="选择你要认证的资质名称">选择你要认证的资质名称</li>		      			<div class="clear"></div>		      		</ul>		      	#else		      		<ul class="zizhiList">		      			#for(com.ekfans.base.store.model.Intel in : ilist)		      				<li title="${in.name}">${in.name}		      					<a href="javascript:void(0);" class="closeLi"></a>		      					<input type="hidden" name="intelId" value="${in.id}" />		      				</li>		      			#end		      		</ul>					      	#end		        <div class="clear"></div>		      </dd>		      <div class="clear"></div>		  	</dl>			<dl class="formItem formLine">				<dt>详细描述</dt>				<dd >					<textarea name="content" disabled style="width:545px;">${sb.content}</textarea>				</dd> 				<dd class="clear"></dd>			</dl>					<div class="clear"></div>	</div>	<script>	$(".areaIdArea").attr("readonly","readonly");	$(".areaIdArea").attr("disabled","disabled");	</script>	 <script type="text/javascript" src="${webroot}/resources/userCenter/commons/js/base.js"></script></body></html>