<!doctype html><html><head><meta name="renderer" content="webkit">	<meta http-equiv="X-UA-Compatible" content="IE=Edge">	<title>发布商品</title>		<script src="${webroot}/resources/plugin/datePicker/WdatePicker.js"></script>		<script src="${webroot}/resources/userCenter/personal/js/ckeditor/ckeditor.js"></script>		<script type="text/javascript" src="${webroot}/resources/commons/js/jquery.form.js"></script>		<script type="text/javascript" src="${webroot}/resources/commons/js/ajaxupload.js"></script>		<link href="${webroot}/resources/userCenter/commons/css/addGoods.css" rel="stylesheet" type="text/css">		<link href="${webroot}/resources/userCenter/commons/css/proAdd.css" rel="stylesheet" type="text/css">		<script type="text/javascript" src="${webroot}/resources/commons/js/upload/upload.js"></script>		<link href="${webroot}/resources/commons/css/tools.css" rel="stylesheet" type="text/css">		<script type="text/javascript" src="${webroot}/resources/commons/js/tools.js"></script>		<!--导入自定义Js-->			<script type="text/javascript" src="${webroot}/resources/userCenter/coreCompany/js/supplyListModefiy.js"></script></head><body>	#define(com.ekfans.base.product.model.SupplyProduct sp)	#define(String type)<form action="${webroot}/store/supply/supplyEdit" name="addOk" method="post"  id="editSupplyProductForm">	<input type="hidden" id="ctxpath" value="${webroot}" />	<input type="hidden" name="id" value="${sp.id}" />	<input type="hidden" name="mainCategory" value="${sp.mainCategory}" />	<input type="hidden" name="createTime" value="${sp.createTime}" />	<input type="hidden" name="userId" value="${sp.userId}" />			<div class="contentTit budong borderColor">			#if(type=="2")				<h3>编辑商品</h3>				<a href="javascript:void(0);" class="btnBase btnSave" onclick="formSubmit();">保存</a>				<a href="javascript:void(0);" class="btnBase btnCancel" onclick="history.go(-1)">返回</a>			#else				<h3>查看商品</h3>				<a href="javascript:void(0);" class="btnBase btnCancel" onclick="history.go(-1)">返回</a>			#end			<div class="clear"></div>		</div>		<div class="form borderColor">			<dl class="formItem">				<dt><em class="mustIco"></em>商品名称</dt>				<dd>				 #if(type=="1")					<input type="text" class="textBox box212" placeholder="1-128位字符组成" disabled name="supplyProductName"  value="${sp.supplyProductName}"/>				 #else					<input type="text" class="textBox box212" placeholder="1-128位字符组成"  name="supplyProductName"  value="${sp.supplyProductName}"/>				 				 #end						<span id="nameSpan" class="errorText" style="display:none;"></span>				</dd>				<dd class="clear"></dd>			</dl>		  		  <dl class="formItem">				<dt><em class="mustIco"></em>商品数量</dt>				<dd>				#if(type=="1")					<input type="text" class="textBox box212" name="quantity" disabled id="quantity" value="${sp.quantity}"/>				#else					<input type="text" class="textBox box212" name="quantity" id="quantity" value="${sp.quantity}"/>				#end										<span id="unitSpan" class="errorText" style="display:none;"></span>				</dd>				<dd class="clear"></dd>			</dl>			<dl class="formItem">				<dt><em class="mustIco"></em>商品编号</dt>				<dd>				#if(type=="1")					<input type="text" class="textBox box212" disabled name="productNum" id="productNum" value="${sp.productNum}"/>				#else					<input type="text" class="textBox box212" name="productNum" id="productNum" value="${sp.productNum}"/>				#end										<span id="unitSpan" class="errorText" style="display:none;"></span>				</dd>				<dd class="clear"></dd>			</dl>				<dl class="formItem">				<dt><em class="mustIco"></em>数量单位</dt>				<dd>				#if(type=="1")					<input type="text" class="textBox box212" disabled name="unit" id="unit" value="${sp.unit}" />				#else					<input type="text" class="textBox box212" name="unit" id="unit" value="${sp.unit}" />				#end										<span id="unitSpan" class="errorText" style="display:none;"></span>				</dd>				<dd class="clear"></dd>			</dl>		  <dl class="formItem">				<dt>批发参考价</dt>				<dd>				#if(type=="1")					<input type="text" class="textBox box212" disabled  name="pfPrice"  value="${sp.pfPrice}" />				#else					<input type="text" class="textBox box212"  name="pfPrice"  value="${sp.pfPrice}" />				#end									</dd>				<dd class="clear"></dd>			</dl>			  <dl class="formItem">				<dt>联系人</dt>				<dd>				#if(type=="1")					<input type="text" class="textBox box212" disabled name="linkPerson" value="${sp.linkPerson}"/>				#else					<input type="text" class="textBox box212"  name="linkPerson" value="${sp.linkPerson}"/>				#end								</dd>				<dd class="clear"></dd>			</dl>  			<dl class="formItem">				<dt>联系方式</dt>				<dd>				#if(type=="1")					<input type="text" class="textBox box212" disabled name="linkTel" value="${sp.linkTel}" />				#else					<input type="text" class="textBox box212"  name="linkTel" value="${sp.linkTel}" />				#end												</dd>				<dd class="clear"></dd>			</dl>			<dl class="formItem">				<dt>上下架</dt>				<dd>				#if(type=="1")					<input type="radio" name="status" value="0" #if(sp.status==0)checked="checked"#end />下架					<input type="radio" name="status" value="1" #if(sp.status==1)checked="checked"#end />上架				#else					<input type="radio" name="status" value="0" #if(sp.status==0)checked="checked"#end />下架					<input type="radio" name="status" value="1" #if(sp.status==1)checked="checked"#end />上架				#end												</dd>				<dd class="clear"></dd>			</dl>			<dl class="formItem formLine">				<dt>商品产地 </dt>				#if(type=="1")						<dd class="select4">#tag systemAreaSel("habitat",sp.habitat) #end</dd>				#else						<dd class="select4">#tag systemAreaSel("habitat",sp.habitat) #end</dd>				#end							<dd class="clear"></dd>			</dl>			<dl class="formItem formLine">				<dt>产地详细地址</dt>				<dd>				#if(type=="1")					<input type="text" name="habitatAddress" disabled class="textBox box560" value="${sp.habitatAddress}">				#else					<input type="text" name="habitatAddress"  class="textBox box560" value="${sp.habitatAddress}">				#end				</dd>				<dd class="clear"></dd>			</dl>			<dl class="formItem formLine">				<dt>详细描述</dt>				<dd  style="height:auto;">					#if(type=="1")				<textarea name="note" disabled style="width:545px;">${sp.note}</textarea>				#else					<textarea name="note" style="width:545px;">${sp.note}</textarea>				#end				</dd> 				<dd class="clear"></dd>			</dl>			<div class="clear"></div>		</div>	</div></form>	</body></html>