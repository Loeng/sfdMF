<!doctype html><html><head><meta name="renderer" content="webkit">	<meta http-equiv="X-UA-Compatible" content="IE=Edge">	<title>发布商品</title>		<script src="${webroot}/resources/plugin/datePicker/WdatePicker.js"></script>		<script src="${webroot}/resources/userCenter/personal/js/ckeditor/ckeditor.js"></script>		<script type="text/javascript" src="${webroot}/resources/commons/js/jquery.form.js"></script>		<script type="text/javascript" src="${webroot}/resources/commons/js/ajaxupload.js"></script>		<link href="${webroot}/resources/userCenter/commons/css/addGoods.css" rel="stylesheet" type="text/css">		<link href="${webroot}/resources/userCenter/commons/css/proAdd.css" rel="stylesheet" type="text/css">		<script type="text/javascript" src="${webroot}/resources/commons/js/upload/upload.js"></script>		<link href="${webroot}/resources/commons/css/tools.css" rel="stylesheet" type="text/css">		<script type="text/javascript" src="${webroot}/resources/commons/js/tools.js"></script>		<!--导入自定义Js-->		<script type="text/javascript" src="${webroot}/resources/userCenter/commons/js/product/supplyAdd.js"></script></head><body>	#define(com.ekfans.base.product.model.ProductTemplate productTemplate)	<input type="hidden" id="ctxpath" value="${webroot}" />	<input type="hidden" id="idCheck" value="false" />	<div class="contentTit budong borderColor">		<h3>发布商品</h3>		<a href="javascript:void(0);" class="btnBase btnSave" onclick="formSubmit();">保存</a>		<a href="javascript:void(0);" class="btnBase btnCancel" onclick="reset();">重置</a>		<div class="clear"></div>	</div>	<div class="form borderColor">		<form action="${webroot}/store/product/savesupply" name="addOk" method="post"  id="addSupplyProductForm">			<input type="hidden" name="brand" id="brand" value="">			<input type="hidden" class="i_bg" name="mainCategory" value="${pcId}" >			<div class="ts bgColor">&nbsp;&nbsp;${pcName}</div>      <a href="${webroot}/store/product/supplyProductAddClassify" class="btnBase">编辑分类</a>			<div class="clear"></div>			<dl class="formItem">				<dt><em class="mustIco"></em>商品编号</dt>				<dd>					<input type="text" class="textBox box212" name="productNum" id="productNum" value=""/>					<span id="unitSpan" class="errorText" style="display:none;"></span>				</dd>				<dd class="clear"></dd>			</dl>			<dl class="formItem">				<dt><em class="mustIco"></em>商品名称</dt>				<dd>					<input type="text" class="textBox box212" placeholder="1-128位字符组成"  name="supplyProductName"  onblur="javascript:checkNameBlur();" onkeyup="javascript:checkName();" onfocus="javascript:checkName();"/>					<span id="nameSpan" class="errorText" style="display:none;"></span>				</dd>				<dd class="clear"></dd>			</dl>		  		  <dl class="formItem">				<dt><em class="mustIco"></em>商品数量</dt>				<dd>					<input type="text" class="textBox box212" name="quantity" id="quantity" value=""/>					<span id="unitSpan" class="errorText" style="display:none;"></span>				</dd>				<dd class="clear"></dd>			</dl>							<dl class="formItem">				<dt><em class="mustIco"></em>数量单位</dt>				<dd>					<input type="text" class="textBox box212" name="unit" id="unit" value="" onblur="checkProductNum();"/>					<span id="unitSpan" class="errorText" style="display:none;"></span>				</dd>				<dd class="clear"></dd>			</dl>		  <dl class="formItem">				<dt>批发参考价</dt>				<dd><input type="text" class="textBox box212"  name="pfPrice"  value="0.00" onpaste="return false" onblur="javascript:if(this.value==''){this.value=0;}" onkeyup="javascript:clearNoNum(this)"/></dd>				<dd class="clear"></dd>			</dl>			  <dl class="formItem">				<dt>联系人</dt>				<dd><input type="text" class="textBox box212"  name="linkPerson" /></dd>				<dd class="clear"></dd>			</dl>  <dl class="formItem">				<dt>联系方式</dt>				<dd><input type="text" class="textBox box212"  name="linkTel"  /></dd>				<dd class="clear"></dd>			</dl>			<dl class="formItem">				<dt>上下架</dt>				<dd>					<span class="radio"><label><input type="radio" name="status" value="0">上架</label></span>					<span class="radio"><label><input type="radio" name="status" value="1" checked>下架</label></span>				</dd>				<dd class="clear"></dd>			</dl>			<dl class="formItem formLine">				<dt>商品产地</dt>				<dd class="select4">#tag systemAreaSel("habitat","") #end</dd>				<dd class="clear"></dd>			</dl>			<dl class="formItem formLine">				<dt>产地详细地址</dt>				<dd><input type="text" name="habitatAddress" class="textBox box560"></dd>				<dd class="clear"></dd>			</dl>						<dl class="formItem formLine">				<dt>详细描述</dt>				<dd  style="height:auto;">					<textarea name="note" style="width:545px;"></textarea>				</dd> 				<dd class="clear"></dd>			</dl>			<div class="clear"></div>		</form>	</div>			<!--关联品牌弹层-->	<div class="apLayerBg"></div>	<div class="apLayer borderColor alertBrand"></div>		<div id="addFieldsDemo" style="display:none;">		<div class="listItem">			<div class="listTd startNum"><input name="startNum" type="text" class="textBox box50" value="0"></div>			<div class="listTd endNum"><input name="endNum" type="text" class="textBox box50" value="0"></div>			<div class="listTd listPrice"><input name="productListPrice" type="text" class="textBox box80" value="0.00"></div>			<div class="listTd listSet"><a href="javascript:void(0);" class="closeIco"></a></div>			<input type="hidden" name="productPrices" class="productPrices">			<div class="clear"></div>		</div>	</div></body></html>