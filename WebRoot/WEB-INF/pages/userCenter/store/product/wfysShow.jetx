<!doctype html><html><head><meta name="renderer" content="webkit">	<meta http-equiv="X-UA-Compatible" content="IE=Edge">	<title>发布需求</title>		<script src="${webroot}/resources/plugin/datePicker/WdatePicker.js"></script>		<script src="${webroot}/resources/userCenter/personal/js/ckeditor/ckeditor.js"></script>		<script type="text/javascript" src="${webroot}/resources/commons/js/jquery.form.js"></script>		<script type="text/javascript" src="${webroot}/resources/commons/js/ajaxupload.js"></script>		<link href="${webroot}/resources/userCenter/commons/css/addGoods.css" rel="stylesheet" type="text/css">		<link href="${webroot}/resources/userCenter/commons/css/proAdd.css" rel="stylesheet" type="text/css">		<link href="${webroot}/resources/userCenter/commons/css/zizhi1.css" rel="stylesheet" type="text/css">		<script type="text/javascript" src="${webroot}/resources/commons/js/upload/upload.js"></script>		<link href="${webroot}/resources/commons/css/tools.css" rel="stylesheet" type="text/css">		<script type="text/javascript" src="${webroot}/resources/commons/js/tools.js"></script>		<script type="text/javascript" src="${webroot}/resources/plugin/datePicker/WdatePicker.js"></script>		<!--导入自定义Js-->		<script type="text/javascript" src="${webroot}/resources/userCenter/commons/js/product/wftransportAdd.js"></script>		<style>		.formItem dt{height:28px;line-height:28px;}		</style></head>#define(com.ekfans.base.store.model.Wftransport wftransport)<body>	<div class="contentTit budong borderColor">		<h3>#if(type == "0")发布车源#else发布货源#end</h3>		<a href="javascript:void(0);" class="btnBase btnSave" onclick="formSubmit();">保存</a>		<a href="javascript:void(0);" class="btnBase btnCancel" onclick="history.go(-1)">返回</a>		<div class="clear"></div>	</div>	<input type="hidden" id="ctxpath" value="${webroot}"/>	<div class="form borderColor">			<dl class="formItem">				<dt><em class="mustIco"></em>#if(wftransport.type == 0)车源标题#else货源标题#end</dt>				<dd>						<input type="text" class="textBox box212" value="${wftransport.name}" placeholder="请输入#if(wftransport.type == 0)车源标题#else货源标题#end"  name="name"  maxlength="64"/>					  	<div class="clear"></div>					  <span id="nameSpan" class="errorText" style="display:none;"></span>				</dd>				<dd class="clear"></dd>			</dl>			<dl class="formItem">				<dt><em class="mustIco"></em>状态</dt>				<dd>					<span class="radio"><label><input type="radio" name="status" value="1" #if(wftransport.status==1)checked#end>上架</label></span>					<span class="radio"><label><input type="radio" name="status" value="0" #if(wftransport.status==0)checked#end>下架</label></span>				</dd>				<dd class="clear"></dd>			</dl>		  <dl class="formItem formLine">				<dt>出发地</dt>				<dd class="select4">#tag systemAreaSel("startPlace",wftransport.startPlace) #end</dd>				<dd class="clear"></dd>			</dl>			<dl class="formItem formLine">				<dt>目的地</dt>				<dd class="select4">#tag systemAreaSel("endPlace",wftransport.endPlace) #end</dd>				<dd class="clear"></dd>			</dl>			<dl class="formItem">				<dt>详细地址</dt>				<dd><input type="text" name="habitatAddress" value="${wftransport.habitatAddress}" class="textBox box212" maxlength="128"></dd>				<dd class="clear"></dd>			</dl>			<dl class="formItem">				<dt><em class=""></em>运价</dt>				<dd>					<input type="text" class="textBox box212" value="${wftransport.price}" placeholder="请输入运价不填为待议"  name="price"  maxlength="16"/>  				 <div class="clear"></div>				</dd>				<dd class="clear"></dd>			</dl>						#if(wftransport.type == 0)				<dl class="formItem">					<dt><em class="mustIco"></em>车辆类型</dt>					<dd>						<input type="text" class="textBox box212" placeholder="请输入车辆类型名称"  value="${wftransport.carName}" name="carName"  onblur="javascript:checkcarNameBlur('${type}');" maxlength="64"/>						  <div class="clear"></div>						<span id="carNameSpan" class="errorText" style="display:none;"></span>					</dd>					<dd class="clear"></dd>				</dl>				<dl class="formItem">					<dt><em class="mustIco"></em>载重</dt>					<dd>						<input type="text" class="textBox box212" placeholder="请输入载重" value="${wftransport.wfpTotal}" name="wfpTotal"  onblur="javascript:checkwfpTotalNameBlur();" maxlength="64"/>吨						  <div class="clear"></div>						<span id="wfpTotalSpan" class="errorText" style="display:none;"></span>					</dd>					<dd class="clear"></dd>				</dl>			#else				<dl class="formItem">					<dt><em class="mustIco"></em>货源名称</dt>					<dd>						<input type="text" class="textBox box212" placeholder="请输入货源名称" value="${wftransport.supplyName}" name="supplyName"  onblur="javascript:checkcarNameBlur('${type}');" maxlength="64"/>						  <div class="clear"></div>						<span id="carNameSpan" class="errorText" style="display:none;"></span>					</dd>					<dd class="clear"></dd>				</dl>				<dl class="formItem">					<dt><em class="mustIco"></em>货源总量</dt>					<dd>						<input type="text" class="textBox box212" placeholder="请输入货源总量"  value="${wftransport.wfpTotal}" name="wfpTotal"  onblur="javascript:checkwfpTotalNameBlur();" maxlength="64"/>						  <div class="clear"></div>						<span id="wfpTotalSpan" class="errorText" style="display:none;"></span>					</dd>					<dd class="clear"></dd>				</dl>			#end		  <dl class="formItem">				<dt><em class="mustIco"></em>联系人</dt>				<dd>					<input type="text" class="textBox box212" placeholder="请输入联系人" value="${wftransport.linkMan}"  name="linkMan"  onblur="javascript:checkContentNameBlur();" maxlength="64"/>					  <div class="clear"></div>					<span id="contactNameSpan" class="errorText" style="display:none;"></span>				</dd>				<dd class="clear"></dd>			</dl>						<dl class="formItem">				<dt><em class="mustIco"></em>联系电话</dt>				<dd>					<input type="text" class="textBox box212" placeholder="请输入联系方式" value="${wftransport.phone}" name="phone"  onblur="javascript:checkContentPhoneBlur();" maxlength="11"/>  				 <div class="clear"></div>  				<span id="contactPhoneSpan" class="errorText" style="display:none;"></span>				</dd>				<dd class="clear"></dd>			</dl>			<dl class="formItem">				<dt>截止时间</dt>				<dd>					<input name="endTime" type="text" placeholder="请输入截止时间不填不限时" value="${wftransport.endTime}" class="textBox box212"  onclick="WdatePicker({dateFmt:'yyyy-MM-dd HH:mm:ss'})" maxlength="19">				</dd>										<dd class="clear"></dd>			</dl>			<dl class="formItem formLine">				<dt>说明</dt>				<dd >					<textarea name="content" style="width:545px;" value="${wftransport.content}" maxlength="200"></textarea>				</dd> 				<dd class="clear"></dd>			</dl>					<div class="clear"></div>		</form>	</div>	</body></html>