        layer.open({
            type: 1,
            title: '发布信息',
            skin: 'layer-class',
            area: ['800px', '600px'], //宽高
            content: $("#fbHtml"),
            btn:['确认','取消'],
            yes:function(index){
                var areaInfo="";
                $(".areaIdArea").each(function(){
                    areaInfo+=$(this).find("option:selected").text()+",";
                });
                $("#destination").val(areaInfo);
                //点击确定执行的语句
                if(checkTitleBlur($("input[name='title']"))&&checkContentNameBlur()&&checkContentPhoneBlur()&&categoryBlur()&&checkNumberBlur()&&futurePricesBlur()&&unitBlur()&&qualityLevelBlur()&&checkAreaBlur()){
                    $("#addForm").ajaxSubmit(function(data){
                        if(data){
                            layer.close(index);
                            layer.alert("发布成功")
                            $("#updateHtml").hide();
                            $("#layui-layer1,#layui-layer-shade1").hide();
                        }else{
                            layer.alert("发布失败!");
                            $("#updateHtml").hide();
                            $("#layui-layer1,#layui-layer-shade1").hide();
                        }
                    });
                }else{
                    checkTitleBlur($("input[name='title']"));
                    checkContentNameBlur();
                    checkContentPhoneBlur();
                    categoryBlur();
                    checkNumberBlur();
                    futurePricesBlur();
                    unitBlur();
                    qualityLevelBlur();
                    checkAreaBlur();
                }
            },
            cancel:function(){
                //点击取消执行的语句
                $("#updateHtml").hide();
                $("#layui-layer1,#layui-layer-shade1").hide();
                window.location.reload();
            }
        });
    });
}
//危废议价和报价   
function wfYijia(id,type,storeId){
        var title = "";
        if("0" == type){
            title = "议价";
        }else{
            title = "报价";
        }
        var userId=$("#userId").val();
        if(userId == storeId){
            layer.alert("不能对自己发布的信息"+title);
            return;
        }
        if(null!=userId&&""!=userId&&" "!=userId || type=="1"){
            $("#yijiaHtml").load($("#ctxpath").val() + "/web/wftransport/getwf/"+id+"/"+type,function(){
                //议价弹层
                layer.open({
                    type: 1,
                    title: title + "信息",
                    skin: 'layer-class',
                    area: '600px', //宽高
                    content: $("#yijiaHtml"),
                    btn:['确认','取消'],
                    yes:function(index){
                        // 判断类型
                        var flag = "false";
                        if(type == '0'){
                            flag = checkQuantity();
                        }else{
                            flag = true;
                        }
                        if( flag ){
                            //点击确定执行的语句
                            if(type == '0' && checkPrice() && checkContactMan() && checkContactPhone()){
                                $("#bargainForm").ajaxSubmit(function(data){
                                    if(data=="1" || data=="true"){
                                        layer.close(index);
                                        layer.alert(title + "成功")
                                    }else if(data=="error"){
                                        layer.close(index);
                                        layer.alert("不能对自己发布的车源议价");
                                    }else{
                                        layer.close(index);
                                        layer.alert(title + "失败!");
                                    }
                                });
                            }
                            if(type=='1' && checkPrice() && checkLinkPerson1() && checkLinkTel1()){
                                $("#bargainForm").ajaxSubmit(function(data){
                                    if(data=="1" || data=="true" ){
                                        layer.close(index);
                                        layer.alert(title + "成功");
                                    }else{
                                        layer.close(index);
                                        layer.alert(title + "失败!");
                                    }
                                });
                            }
                        }else{
                            if(type == '0'){
                                checkPrice();
                                checkQuantity();
                                checkContactMan();
                                checkContactPhone();
                            }else{
                                checkPrice();
                                checkLinkPerson1();
                                checkLinkTel1(); 
                            }
                        }
                    },
                    cancel:function(){
                        //点击取消执行的语句
                    }
                });
            });
        }else{ //未登录跳转登录页面
            location.href=$("#ctxpath").val()+"/web/login";
        }
    }   
    
//加载报价信息
function loadBaojia(id,storeId){
    var ctxpath = $("#ctxpath").val();
    var userId=$("#userId").val();
        if(userId == storeId && storeId != ""){
            layer.alert("不能对自己发布的信息操作");
            return;
        }
    //弹层
    $("#baojiaHtml").load(ctxpath+"/web/supply/loadBaojia",{id:id},function(){
        layer.open({
            type: 1,
            title: '报价信息',//商品标题
            skin: 'layer-class',
            area: '730px', //宽高
            content: $("#baojiaHtml"),
            btn:['确认','取消'],
            yes:function(index){
            if(checkFinalPrice()&&checkStoreName()&&checkLinkPerson()&&checkLinkTel()){
