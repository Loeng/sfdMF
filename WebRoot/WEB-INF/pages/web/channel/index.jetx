function loadMetalChat(metalId,metalName,spec,price,startPrice,endPrice,unit){
    var titleStr = "<em>" + metalName + "</em>" + price + "(" +startPrice + "-" + endPrice + ")" + unit;
    $("#chatShowTitle").html(titleStr);



    // 基于准备好的dom，初始化echarts实例
    var myChart = echarts.init(document.getElementById('metalChart'));
    // 指定图表的配置项和数据
    myChart.showLoading();
    var radomId = "dt" + new Date().getTime();
    $.post('${webroot}/web/metal/hots/jsonload?time='+radomId,{metalId:metalId},function (data){
      data = JSON.parse(data);
      console.log(data);
      var maxValue = parseInt(data[0].value);
      var minValue = parseInt(data[0].value);
        for(i=0;i<data.length;i++){
            var currValue = parseInt(data[i].value);
            if(maxValue<currValue){
                maxValue=currValue;
            }
            if(minValue>currValue){
                minValue=currValue;
            }
          }
          var zone=parseInt(maxValue*0.01);
          maxValue=maxValue+zone+1;
          if(minValue-zone>0){
            minValue=minValue-zone;
          }
      myChart.hideLoading();      
      myChart.showLoading({text: "图表数据正在努力加载..."}); 
      myChart.setOption(
        option = {
          color:['#F1457D'],
					grid:{
						x:75,
						y:30,
						x2:30,
						y2:50
					},
					calculable : false,
          tooltip : {
            trigger: 'axis'
          },
          xAxis : [{
            type : 'category',
            boundaryGap : false,
            splitNumber:1,
            data: data.map(function (item) {
              return item.date;
            }),
            axisLine:{
							show:true,
							lineStyle:{
								color:'#eeefef',
								width:1
							}
						},
						splitLine : {    // 轴线
							show: true,
							lineStyle:{
								color:'#eeefef'
							}
						},
						axisLabel : {
							show:true,
							interval: 'auto',    // {number}
							margin: 8,
							textStyle: {
								color: '#333'
							}
						},
						axisTick : {    // 轴标记
							show:false,
						}
          }],
          yAxis : [{
            type : 'value',
            scale:false,
            min: minValue,
            max: maxValue,
            splitNumber:10,
						axisLine:{
							show:true,
							lineStyle:{
								color:'#eeefef',
								width:1
							}
						},
						splitLine : {    // 轴线
							show: true,
							lineStyle:{
								color:'#eeefef'
							}
						},
						axisLabel : {
							show:true,
							interval: 'auto',    // {number}
							margin: 8,
							textStyle: {
								color: '#f00'
							}
						},
						axisTick : {    // 轴标记
							show:false,
						}
          }],
          series : [{
            name:'当前行情',
            type:'line',
            data: data.map(function (item) {
              return item.value;
            }),
            symbol: 'circle',
						symbolSize:6,
            itemStyle: {
							normal: {
								label : {
									show : false,
									position:'top',
									textStyle : {
										color : '#fff',
										fontSize : '12',
									},
								}
							}
						},
          }]
        }
      );

      myChart.hideLoading();

      
    });
    $("#"+metalId).addClass("cur").siblings().removeClass("cur");
    
}

