<!doctype html><html><head>	<title>发布商品</title>		<script type="text/javascript" src="${webroot}/resources/commons/js/jquery-1.8.3.min.js"></script>		<script type="text/javascript" src="${webroot}/resources/commons/js/jquery.form.js"></script>		<script type="text/javascript" src="${webroot}/resources/commons/js/ajaxupload.js"></script>		<script type="text/javascript" src="${webroot}/resources/plugin/ckeditor/ckeditor.js"></script>		<script type="text/javascript" src="${webroot}/resources/system/js/tools.js"></script>		<script type="text/javascript" src="${webroot}/resources/system/js/base.js"></script>		<link href="${webroot}/resources/system/css/product/issue.css" rel="stylesheet" type="text/css" />		<link href="${webroot}/resources/system/css/list.css" rel="stylesheet" type="text/css" />		<!--导入自定义Js-->			<script type="text/javascript" src="${webroot}/resources/system/js/product/supplyListModefiy.js"></script></head><body>	#define(com.ekfans.base.product.model.SupplyProduct sp)	#define(String type)	<input type="hidden" id="ctxpath" value="${webroot}" />	<input type="hidden" id="idCheck" value="false" />	<div class="ht_btn">		#if(type=="2")				<a href="javascript:void(0);" class="btn" onclick="formSubmit();">保存</a>				<a href="javascript:void(0);" class="btn" onclick="history.go(-1)">返回</a>			#else							<a href="javascript:void(0);" class="btn" onclick="history.go(-1)">返回</a>			#end	</div>	<div class="form borderColor">		<form action="${webroot}/system/supply/supplyEdit" name="addOk" method="post"  id="addSystemSupplyProductForm">			<input type="hidden" name="id" id="brand" value="${sp.id}">			<input type="hidden" name="mainCategory" id="brand" value="${sp.mainCategory}">      <div class="add_pro">			<div class="ts">	    	<a href="javascript:void(0);">${pcName}</a>&nbsp;&nbsp;	    </div>			<dl class="formBox">				<dt><em class="mustIco"></em>商品名称</dt>				<dd>					<input id="productName" type="text" class="i_bg" placeholder="1-128位字符组成"  name="supplyProductName" value="${sp.supplyProductName}"/>					<span id="nameSpan" class="errorText" style="display:none;">商品名称不能为空</span>				</dd>				<dd class="clear"></dd>			</dl>		  <dl class="formItem">				<dt><em class="mustIco"></em>商品数量</dt>				<dd>					<input type="text" class="i_bg"  name="quantity" id="quantity" value="${sp.quantity}"/>					<span id="unitSpan" class="errorText" style="display:none;">数量不能为空,或请输入正确数量</span>				</dd>				<dd class="clear"></dd>			</dl>			<dl class="formItem">				<dt><em class="mustIco"></em>商品编号</dt>				<dd>					<input type="text" class="i_bg"  name="productNum" id="productNum" value="${sp.productNum}"/>				</dd>				<dd class="clear"></dd>			</dl>				<dl class="formItem">				<dt><em class="mustIco"></em>数量单位</dt>				<dd>					<input type="text" class="i_bg"  name="unit" id="unit" value="${sp.unit}" >									</dd>				<dd class="clear"></dd>			</dl>		  <dl class="formItem">				<dt><em class="mustIco"></em>批发参考价</dt>				<dd>					<input id="jiage" type="text" class="i_bg"   name="pfPrice"  value="${sp.pfPrice}"/>					<span id="jiageSpan" class="errorText" style="display:none;">批发价不为空,或请输入正确价格</span>					</dd>				<dd class="clear"></dd>			</dl>			  <dl class="formItem">				<dt><em class="mustIco"></em>联系人</dt>				<dd>					<input id="linkPerson" type="text" class="i_bg"  name="linkPerson" value="${sp.linkPerson}" />					<span id="linkPersonSpan" class="errorText" style="display:none;">联系人不为空</span>				</dd>				<dd class="clear"></dd>			</dl>  		<dl class="formItem">				<dt><em class="mustIco"></em>联系方式</dt>				<dd>					<input id="linkTel" type="text" class="i_bg"   name="linkTel" value="${sp.linkTel}" />						<span id="linkTelSpan" class="errorText" style="display:none;">联合方式不为空,或请输入正确的联系方式</span>				</dd>				<dd class="clear"></dd>			</dl>						<dl>			  <dt><em class="mustIco"></em>选择发布人</dt>			  <dd>			 	  <select id="name" name="userId" class="i_bg"  style="width:157px;">			 	  	<option  value ="">请选择发布人</option>	            #for(com.ekfans.base.user.model.User u:user)		            #if(u.id == sp.userId)		            	 #if(u.nickName=="")		            	 	 <option selected value ="${u.id}">${u.name}</option>		            	 #else		            	   <option selected value ="${u.id}">${u.nickName}</option>		            	 #end		            #else		            	 #if(u.nickName=="")		            	 	 <option value ="${u.id}">${u.name}</option>		            	 #else		            	   <option value ="${u.id}">${u.nickName}</option>		            	 #end		            #end	            #end	              	</select>			 	</dd>				</dl>			<dl class="formItem formLine">				<dt>商品产地</dt>				<dd class="select4" >#tag systemAreaSel("habitat",sp.habitat) #end</dd>				<dd class="clear"></dd>			</dl>			<dl class="formItem formLine">				<dt>产地详细地址</dt>				<dd><input type="text" name="habitatAddress" class="i_bg"  value="${sp.habitatAddress}"></dd>				<dd class="clear"></dd>			</dl>						<dl class="formItem formLine">				<dt>详细描述</dt>				<dd >					<textarea name="note" class="i_bg"  style="width:545px;">${sp.note}</textarea>				</dd> 				<dd class="clear"></dd>			</dl>			<div class="clear"></div>		</form>	</div>			<!--关联品牌弹层-->	<div class="apLayerBg"></div>	<div class="apLayer borderColor alertBrand"></div>		<div id="addFieldsDemo" style="display:none;">		<div class="listItem">			<div class="listTd startNum"><input name="startNum" type="text" class="textBox box50" value="0"></div>			<div class="listTd endNum"><input name="endNum" type="text" class="textBox box50" value="0"></div>			<div class="listTd listPrice"><input name="productListPrice" type="text" class="textBox box80" value="0.00"></div>			<div class="listTd listSet"><a href="javascript:void(0);" class="closeIco"></a></div>			<input type="hidden" name="productPrices" class="productPrices">			<div class="clear"></div>		</div>	</div></body></html>