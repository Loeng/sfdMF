	FREIGHT decimal(18,4) DEFAULT 0.00 COMMENT '总运费',
	COUNT_TYPE CHAR(1) DEFAULT '0' COMMENT'计价类型(是否一口价) 0不是一口价 1一口价', 
	CONTRACT_ORIGINAL decimal(18,4) DEFAULT 0.00 COMMENT'合同单价', 
