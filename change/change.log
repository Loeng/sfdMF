DROP TABLE IF EXISTS `wfpys`;
CREATE TABLE `wfpys` (
  `ID` varchar(32) NOT NULL DEFAULT '' COMMENT '主键',
  `NAME` varchar(512) DEFAULT NULL COMMENT '名称',
  `DESCR` varchar(512) DEFAULT NULL COMMENT '描述',
  `PARENT_ID` varchar(32) DEFAULT NULL COMMENT '父级id',
  `FULL_PATH` varchar(256) DEFAULT NULL COMMENT '关联id',
  `POSITION` int(11) DEFAULT '0' COMMENT '排序'
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

INSERT INTO `wfpys` VALUES ('001000000', '一类：爆炸品', '本类化学品系指在外界作用下（如受热、受压、撞击等），能发生剧烈的化学反应，瞬时产生大量的气体和热量，使周围压力急骤上升，发生爆炸，对周围环境造成破坏的物品，也包括无整体爆炸危险，但具有燃烧、抛射及较小爆炸危险，或仅产生热、光、音响或烟雾等一种或几种作用的烟火物品。', null, '001000000|', '0');
INSERT INTO `wfpys` VALUES ('002000000', '二类：压缩气体和液化气体', '本类化学品系指压缩、液化或加压溶解的气体。', null, '002000000|', '1');
INSERT INTO `wfpys` VALUES ('002000001', '易燃气体', '此类气体极易燃烧，与空气混合能形成爆炸性混合物。', '002000000', '002000000|002000001', '0');
INSERT INTO `wfpys` VALUES ('002000002', '不燃气体', '不燃气体系指无毒、不燃气体、包括助燃气体。', '002000000', '002000000|002000002', '1');
INSERT INTO `wfpys` VALUES ('002000003', '有毒气体', '该类气体有毒，毒性指标与第6类毒性指标相同。对人畜有强烈的毒害、窒息、灼伤、刺激作用。其中有些还具有易燃、氧化、腐蚀等性质。', '002000000', '002000000|002000003', '2');
INSERT INTO `wfpys` VALUES ('003000000', '三类：易燃液体', '本类化学品系指易燃的液体、液体混合物或含有固体物质的液体，但不包括由于其危险性已列入其它类别的液体。其闭杯闪点等于或低于61℃。', null, '003000000|', '2');
INSERT INTO `wfpys` VALUES ('003000001', '低闪点液体', '指闭杯闪点低于－18℃的液体 ；', '003000000', '003000000|003000001', '0');
INSERT INTO `wfpys` VALUES ('003000002', '中闪点液体', '指闭杯闪点在－18℃至23℃的液体；', '003000000', '003000000|003000002', '1');
INSERT INTO `wfpys` VALUES ('003000003', '高闪点液体', '指闭杯闪点在23℃至61℃的液体。', '003000000', '003000000|003000003', '2');
INSERT INTO `wfpys` VALUES ('004000000', '四类：易燃固体、自燃物品和遇湿易燃物品', '', null, '004000000|', '3');
INSERT INTO `wfpys` VALUES ('004000001', '易燃固体', '本项化学品系指燃点低、对热、撞击、摩擦敏感，易被外部火源点燃，燃烧迅速，并可能散发出有毒烟雾或有毒气体的固体，但不包括已列入爆炸品的物质。', '004000000', '004000000|004000001', '0');
INSERT INTO `wfpys` VALUES ('004000002', '自燃物品', '本项化学品系指自燃点低，在空气中易于发生氧化反应，放出热量，而自行燃烧的物品。', '004000000', '004000000|004000002', '1');
INSERT INTO `wfpys` VALUES ('004000003', '遇湿易燃物品', '本项化学品系指遇水或受潮时，发生剧烈化学反应，放出大量的易燃气体和热量的物品有些不需明火，即能燃烧或爆炸。', '004000000', '004000000|004000003', '2');
INSERT INTO `wfpys` VALUES ('005000000', '五类：氧化剂和有机过氧化物', '', null, '005000000|', '4');
INSERT INTO `wfpys` VALUES ('005000001', ' 氧化剂', '氧化剂系指处于高氧化态，具有强氧化性，易分解并放出氧和热量的物质。包括含有过氧基的有机物，其本身不一定可燃，但能导致可燃物的燃烧；与松软的粉末状可燃物能组成爆炸性混合物，对热、震动或摩擦较为敏感。', '005000000', '005000000|005000001', '0');
INSERT INTO `wfpys` VALUES ('005000002', '有机过氧化剂', '有机过氧化物系指分子组成中含有过氧基的有机物，其本身易燃易爆、极易分解，对热、震动和摩擦极为敏感。', '005000000', '005000000|005000002', '1');
INSERT INTO `wfpys` VALUES ('006000000', '六类：毒害品和感染性物品', '', null, '006000000|', '5');
INSERT INTO `wfpys` VALUES ('006000001', ' 毒害品', '具体指标 经口：LD50≤500 mg/kg（固体）;LD50≤2000 mg/kg（液体）;经皮：LD50≤1000 mg/kg(24hr接触);吸入：LC50≤10 mg/L（粉尘、烟雾、蒸气）', '006000000', '006000000|006000001', '0');
INSERT INTO `wfpys` VALUES ('006000002', '感染性物品', '本项化学品系指含有致病的微生物，能引起病态，甚至死亡的物质。', '006000000', '006000000|006000002', '1');
INSERT INTO `wfpys` VALUES ('007000000', '七类：放射性物品', '本类化学品系指放射性比活度大于7.4×10Bq/kg的物品。', null, '007000000|', '6');
INSERT INTO `wfpys` VALUES ('008000000', '八类：腐蚀品', '本类化学品系指能灼伤人体组织并对金属等物品造成损坏的固体或液体。与皮肤接触在4小时内出现可见坏死现象，或温度在55℃时，对20号钢的表面均匀年腐蚀超过6.25 mm的固体或液体。', null, '008000000|', '7');
INSERT INTO `wfpys` VALUES ('008000001', '酸性腐蚀品', '', '008000000', '008000000|008000001', '0');
INSERT INTO `wfpys` VALUES ('008000002', '碱性腐蚀品', '', '008000000', '008000000|008000002', '1');
INSERT INTO `wfpys` VALUES ('008000003', '其它腐蚀品', '', '008000000', '008000000|008000003', '2');
INSERT INTO `wfpys` VALUES ('009000000', '九类：杂类危险品', '', null, '009000000|', '8');

