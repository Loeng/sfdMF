	TOTAL_PRICE  decimal(18,4) DEFAULT 0.00 COMMENT'订单总价',
	REAL_PRICE decimal(18,4) DEFAULT 0.00 COMMENT'最终单价（如果是一口价，合同原价+微量元素；如果非一口价，则是微量元素的总和）', 
	REAL_TOTAL_PRICE decimal(18,4) DEFAULT 0.00 COMMENT '订单最终总价',
	YF_PRICE decimal(18,4) DEFAULT 0.00 COMMENT'预付金额', 
	PAY_PRICE decimal(18,4) DEFAULT 0.00 COMMENT'已付金额', 
	PAY_STATUS CHAR(1) DEFAULT '0'COMMENT '支付状态 0:待支付 1:已支付预付金 2:已全额支付',
	STATUS CHAR(2) DEFAULT '00' COMMENT '订单状态 00新下单待确认运输企业 01已确认运输企业待处置企业确认订单 02已确认订单待运输企业确认运输车辆信息 03待支付预付金 04已支付待发货 05已发货待收货 06已收货 07待卖家确认品位单以及品味含量 08已确认品味信息待双方确认金额 09待最终支付 10订单完成',
	BUY_SURE_STATUS CHAR(1) default '0' COMMENT '买家确认品味状态',
	SALE_SURE_STATUS CHAR(1) default '0' COMMENT '卖家确认品味状态',
	SEND_ADDRESS  VARCHAR(320) DEFAULT NULL COMMENT'发货地址',
	SEND_LINK_MAN VARCHAR(32) DEFAULT NULL COMMENT'发货联系人',
	SEND_LINK_PHONE VARCHAR(32) DEFAULT NULL COMMENT'发货联系方式',
	TAKE_ADDRESS  VARCHAR(320) DEFAULT NULL COMMENT'收货地址',
	TAKE_LINK_MAN VARCHAR(32) DEFAULT NULL COMMENT'收货联系人',
	TAKE_LINK_PHONE VARCHAR(32) DEFAULT NULL COMMENT'收货联系方式',
	TEST_REPORT varchar(128) default NULL COMMENT '检测报告',
	LABORATORY varchar(320) default null COMMENT '最终化验单',
	CREATE_TIME VARCHAR(32) DEFAULT NULL COMMENT'创建时间',
	UPDATE_TIME VARCHAR(32) DEFAULT NULL COMMENT'创建时间'
)default charset=utf8;

