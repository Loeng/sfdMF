-- 刘国羽 合同表重建
DROP TABLE IF EXISTS `contract`;
CREATE TABLE `contract` (
  `Id` varchar(32) NOT NULL COMMENT '主键',
  `CONTRACT_NAME` varchar(32) DEFAULT NULL COMMENT '契约名称',
  `CONTRACT_NO` varchar(64) DEFAULT NULL COMMENT '契约编号',
  `TYPE` char(1) DEFAULT '0' COMMENT '契约类型0销售契约 1 运输契约',
  `FIRST_ID` varchar(32) DEFAULT NULL COMMENT '契约甲方',
  `SECOND_ID` varchar(32) DEFAULT NULL COMMENT '契约已方',
  `START_TIME` varchar(19) DEFAULT NULL COMMENT '开始时间',
  `END_TIME` varchar(19) DEFAULT NULL COMMENT '结束时间',
  `QUANTITY` double(13,4) DEFAULT 0.00 COMMENT '处理总量',
  `CREATOR` varchar(32) DEFAULT NULL COMMENT '创建人',
  `FILE` varchar(128) DEFAULT NULL COMMENT '契约附件',
  `CREATE_TIME` varchar(19) DEFAULT NULL COMMENT '创建时间',
  `UPDATE_TIME` varchar(19) DEFAULT NULL COMMENT '修改时间',
  `NOTE` varchar(512) DEFAULT NULL COMMENT '备注',
  `ORG_ID` varchar(128) DEFAULT NULL COMMENT '组织结构',
  `IS_DEL` char(1) DEFAULT NULL,
  `CONTRACT_CHECK_STATUS` char(1) DEFAULT '0' COMMENT '契约审核状态',
  `PARTYAORPARTYB` char(1) DEFAULT '0' COMMENT '甲方or乙方选择运输企业  0甲方  1乙方',
  `CONTRACT_STATUS` char(1) DEFAULT NULL COMMENT '契约状态 0启用1不启用',
  `FREIGHT_TYPE` char(1) DEFAULT '0' COMMENT '运费由哪方承担 0甲方  1乙方',
  `WF_TYPE` char(1) DEFAULT '0' COMMENT '危废类型 0固态  1液态  2 半固态 3其他',
  `MARGIN_SCALE` double(11,4) DEFAULT 0.00 COMMENT '保证金比例',
  `PAYTYPE` varchar(1) DEFAULT NULL COMMENT '付费方式（一口价、含量计算）',
  `YKJPRICE` varchar(8) DEFAULT NULL COMMENT '一口价',
  PRIMARY KEY (`Id`),
  UNIQUE KEY `CONTRACT_INDEX` (`CONTRACT_NO`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='契约表（合同表）';


